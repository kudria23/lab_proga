#include <stdio.h>

int main() {
    int t1, t2, t3;
    scanf("%d %d %d", &t1, &t2, &t3);

    // Обчислюємо загальний час в годинах
    float total_time = (float)(t1 + t2 + t3) / 60.0;

    // Виводимо результат з округленням до 2 знаків після коми
    printf("%.2f", total_time);

    return 0;
}
